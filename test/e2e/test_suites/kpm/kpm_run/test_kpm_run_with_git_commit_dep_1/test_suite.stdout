a:
  workload:
    containers:
      nginx:
        image: nginx:v1
        command:
        - /bin/sh
        - -c
        - echo hi
        args:
        - /bin/sh
        - -c
        - echo hi
        env:
          env1: VALUE
          env2: secret://sec-name/key
        workingDir: /tmp
        readinessProbe:
          probeHandler:
            url: http://localhost:80
          initialDelaySeconds: 10
    replicas: 2
  accessories:
    test: test
